  <connectionStrings>
    <!--Настройки за връзка към базата данни
    Data Source = идентификатор от TNSNames.ora файла за достъп до базата данни.
    User id = потребител 
    Password = парола
    -->
    <add name="defaultRWConnectionString" connectionString="User Id=epzeu_dev;Password=epzeu_dev;Host=vm-av-epzeu-db1.dev.local;Port=5432;Database=epzeu_dev;" providerName="Npgsql" />
  </connectionStrings>
  <appSettings>
