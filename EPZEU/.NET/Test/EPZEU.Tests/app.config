    <add key="Emails:Interval" value="00:00:30" />

    <add key="EPZEU:ApiRoot" value="https://localhost/EPZEU.Web.Api/" />
    <add key="ComReg:ApiBaseAddress" value="http://ws-soft53.cnsys.plc/Integration.EPZEU.Api/api/" />
