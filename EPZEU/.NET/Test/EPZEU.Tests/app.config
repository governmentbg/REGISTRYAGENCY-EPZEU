  <!-- Summary: 
            List of SMTP Providers, used by the email sender engine. 
            Parameters:
              smtpSecurity
              Values:
                    Unsecure,       Initially unsecured connection. 
                    Implicit,       Implicitly secured connection.     
                    Secure,         Explicitly secured connection. Same as Secure. 
                    Explicit        Explicitly secured connection. Same as Explicit. 
                    
              smtpDefaultPort
              Values:
                    Explicit SSL Protection = 25,
                    Implicit SSL Protection = 465
                    
              smtpAuthentication
              Values:
                  Auto,
                  Plain,
                  DigestMD5,
                  CramMD5,
                  Login,
                  Ntlm,
                  GssApi

              NetworkUser,
              NetworkPass,
              EmailFrom,
	                Summary:
		              Network Creadentials, that should be passed to the SMTP Host.
	-->
  <SMTPProviderConfiguration>
    <SMTPProviders>
      <add name="public" smtpHost="devmail.dev.local" smtpSecurity="Unsecure" smtpDefaultPort="25" smtpAuthentication="Auto" networkUser="dev\mailsender" networkPass="R1r2r3r4" emailFrom="mailsender@dev.cnsys.bg"></add>
      <add name="private" smtpHost="devmail.dev.local" smtpSecurity="Unsecure" smtpDefaultPort="25" smtpAuthentication="Auto" networkUser="dev\mailsender" networkPass="R1r2r3r4" emailFrom="mailsender@dev.cnsys.bg"></add>
    </SMTPProviders>
  </SMTPProviderConfiguration>

  <system.data>
    <DbProviderFactories>
      <add name="Npgsql Data Provider" invariant="Npgsql" description=".Net Data Provider for PostgreSQL" type="Npgsql.NpgsqlFactory, Npgsql, Culture=neutral, PublicKeyToken=5d8b90d52f46fda7" />
    </DbProviderFactories>
  </system.data>
