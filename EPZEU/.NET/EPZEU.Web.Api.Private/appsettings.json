{
  "EPZEU": {
    "httpClients": {
      "integration.epzeu.api": {
        "auth": {
          "requiredScopes": "integration.epzeu.api",
          "authenticationClientID": "defaultClient",
          "DelegateIfHasSubject": "true"
        }
      },
      "epzeu.cr.api": {
        "auth": {
          "requiredScopes": "epzeu.cr.api",
          "authenticationClientID": "defaultClient",
          "DelegateIfHasSubject": "true"
        }
      },
      "epzeu.pr.api": {
        "auth": {
          "requiredScopes": "epzeu.pr.api",
          "authenticationClientID": "defaultClient",
          "DelegateIfHasSubject": "true"
        }
      },
      "payments.api": {
        "auth": {
          "requiredScopes": "api.payments.obligations api.payments.transactions api.payments.reporting api.payments.paymentorders",
          "authenticationClientID": "defaultClient",
          "DelegateIfHasSubject": "true"
        }
      }
    },
    "httpAuthenticationClients": {
      "clients": {
        "defaultClient": {
          "ClientID": "epzeu.api.client",
          "ClientSecret": "epzeu.api.client.secret"
        }
      }
    },
    "Authentication": {
      "ApiName": "epzeu.api"
    }
  },
  "Serilog": {
    "Using": [ "EPZEU.Web.Api" ],
    "MinimumLevel": {
      "Default": "Warning"
    }
  }
}
