{
  "EPZEU": {
    "httpClients": {
      "integration.epzeu.api": {
        "auth": {
          "requiredScopes": "integration.epzeu.api",
          "authenticationClientID": "defaultClient",
          "DelegateIfHasSubject": "true"
        }
      },
      "epzeu.cr.api": {
        "auth": {
          "requiredScopes": "epzeu.cr.api",
          "authenticationClientID": "defaultClient",
          "DelegateIfHasSubject": "true"
        }
      },
      "epzeu.pr.api": {
        "auth": {
          "requiredScopes": "epzeu.pr.api",
          "authenticationClientID": "defaultClient",
          "DelegateIfHasSubject": "true"
        }
      },
      "payments.api": {
        "auth": {
          "requiredScopes": "api.payments.obligations.ro api.payments.transactions.ro api.payments.reporting api.payments.paymentorders",
          "authenticationClientID": "defaultClient",
          "DelegateIfHasSubject": "true"
        }
      }
    },
    "httpAuthenticationClients": {
      "clients": {
        "defaultClient": {
          "ClientID": "epzeu.api.client",
          "ClientSecret": "epzeu.api.client.secret"
        }
      }
    },
    "Authentication": {
      "ApiName": "epzeu.api.public",
      "ApiSecret": "epzeu.api.public.secret",
      "EnableCaching": true,
      "CacheDuration": "0:10:00"
    }
  },
  "Swagger": {
    "IPFiltering": {
      "DefaultBlockLevel": "All",
      "HttpStatusCode": 404,
      "Whitelist": [ "::1" ]
    }
  },
  "Serilog": {
    "Using": [ "EPZEU.Web.Api" ],
    "MinimumLevel": {
      "Default": "Warning"
    }
  }
}
