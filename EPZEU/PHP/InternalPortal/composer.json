{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "ext-pdo_pgsql": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-ldap": "*",
        "zendframework/zend-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.1",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-form": "^2.11",
        "zendframework/zend-db": "^2.9",
        "zendframework/zend-session": "^2.8",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-math": "^3.0",
        "zendframework/zend-json": "^3.1",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-cache": "^2.8",
        "zendframework/zend-serializer": "^2.8",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
        "zendframework/zend-permissions-rbac": "^3.0",
        "zendframework/zend-mime": "^2.7",
        "zendframework/zend-ldap": "^2.10",        
        "zendframework/zend-http": "^2.8",
        "phpseclib/phpseclib": "^2.0",
        "box/spout": "^2.7",
        "phpoffice/phpspreadsheet": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "User\\": "module/User/src/",
            "DeclarationTemplate\\": "module/DeclarationTemplate/src/",
			"Document\\": "module/Document/src/",
			"Nomenclature\\": "module/Nomenclature/src/",
			"Forum\\": "module/Forum/src/",
			"Content\\": "module/Content/src/",
			"Message\\": "module/Message/src/",
			"Payment\\": "module/Payment/src/",
			"Statistic\\": "module/Statistic/src/"
        },
        "files": [ "lib/functions.php" ]
    }
}
