<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://www.registryagency.bg/schemas/property-register/segments"
            xmlns="http://www.registryagency.bg/schemas/property-register/segments"
            xmlns:val="http://www.registryagency.bg/schemas/property-register/values"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xsd:include schemaLocation="PeriodForReport.xsd"/>

    <xsd:import namespace="http://www.registryagency.bg/schemas/property-register/values" schemaLocation="../value/Values.xsd"/>

    <xsd:complexType name="PeriodForCertificate">
        <xsd:annotation>
            <xsd:documentation xml:lang="bg">Период от време за издаване на удостоверение</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="PeriodForReport" type="PeriodForReport" />
            <xsd:element name="ExpectedRegistrationDate" type="val:ExpectedRegistrationDate" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
