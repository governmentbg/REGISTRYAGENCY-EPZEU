<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://www.registryagency.bg/schemas/property-register/values"
            xmlns="http://www.registryagency.bg/schemas/property-register/values"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xsd:simpleType name="CadastralId">
        <xsd:annotation>
            <xsd:documentation xml:lang="bg">Кадастрален идентификатор</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9.]+"/>
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>
