{
  "compilerOptions": {
    "baseUrl": "./ClientApp",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "noImplicitAny": true,
    "skipDefaultLibCheck": true,
    //"strict": true,
    "types": [ "webpack-env" ],
    "lib": [ "dom", "es5", "es2015.iterable", "scripthost" ],
    "experimentalDecorators": true
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
